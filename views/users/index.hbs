    <!-- Product Section Begin -->
    <section class="product spad">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <ul class="filter__controls">
                        <li class="active" data-filter="*">Best Sellers</li>
                        <li data-filter=".new-arrivals">New Arrivals</li>
                        <li data-filter=".hot-sales">Hot Sales</li>
                    </ul>
                </div>
            </div>
            <div class="row product__filter">
                     {{#each product}}    
                <div class="col-lg-3 col-md-6 col-sm-6 col-md-6 col-sm-6 ">
                     <a href="/product-details/{{this._id}}"> 
                    <div class="product__item" style="width: 18rem;" >
                        <div class="product__item__pic set-bg ">   
                      <img style="" src="/images/siteproducts/{{this.productimage.[0]}}" alt="{{this.name}}">
                        </div>
                        <br>
                        <div class="product__item__text mt-4">
                             <p class="mt-2">{{this.productbrand}}</p>
                            <h5 class="mt-2">{{this.category}}</h5>
                            <h5 class="mt-2">{{this.productname}}</h5>
                            <p class="mt-2">Original Price :<s class="text-danger">{{this.originalprice}}</s></p>
                           <p class="mt-2">Offer Price: <span class="text-success">{{this.productprice}}</span></p>

                        </div>
                        <br>
                   <a href="/addtoCart/{{product._id}}" class="primary-btn">add to cart</a>
                    </div>
                    </a>     
                </div>
                 {{/each}}
            </div>  
<div>
  {{#if totalPages}}
    <ul class="pagination">
      {{#if (gt currentPage 1)}}
        <li class="page-item"><a class="page-link" href="/?page={{dec currentPage}}">Previous</a></li>
      {{/if}}

      {{#each pages}}
        <li class="page-item{{#eq this currentPage}} active{{/eq}}">
          <a class="page-link" href="/?page={{this}}">{{this}}</a>
        </li>
      {{/each}}

      {{#if (lt currentPage totalPages)}}
        <li class="page-item"><a class="page-link" href="/?page={{inc currentPage}}">Next</a></li>
      {{/if}}
    </ul>
  {{/if}}
</div>


        
